{
	"name": "ttleague",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"app": "next dev --turbo",
		"triplit-start-server": "./bin/run-triplit-local.sh",
		"triplit-push-schema": "sleep 2 && ./bin/triplit-push-schema.sh",
		"triplit-seed": "./bin/triplit-seed-local.sh",
		"triplit-dev": "run-p triplit-start-server triplit-push-schema",
		"dev": "run-p app triplit-dev",
		"build": "bun run triplit-push-schema && next build",
		"build:worker": "cloudflare",
		"dev:worker": "wrangler dev --port 8771",
		"preview:worker": "bun run build:worker && bun run dev:worker",
		"deploy:worker": "bun run build:worker && wrangler deploy",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage"
	},
